fn strlen(str: &char) -> num {
    for (let i: num=0; str[i]; i=i+1;) {}
    i;
}

fn strcpy(dst: &char, src: &char) -> void {
    for (let i: num=0; src[i]; i=i+1;) {
        dst[i] = src[i];
    }
    dst[i] = 0;
}

fn strcat(dst: &char, src: &char) -> void {
    for (let i: num=strlen(dst)-1; src[i]; i=i+1;) {
        dst[i] = src[i];
    }
    dst[i] = 0;
}

fn main() -> void {
    let s: &char = "test";
    prs!(s); prend!();
    strcat(s, "ing");
    prs!(s); prend!();
}